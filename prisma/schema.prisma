// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Swap {
  id      Int      @id @default(autoincrement())
  chatId  BigInt
  from    String
  to      String
  amount  String
  date    DateTime @default(now())
  txId    String?
}
